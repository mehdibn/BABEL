# Kibana is served by a back end server. This setting specifies the port to use.
#server.port: 5601

# Specifies the address to which the Kibana server will bind. IP addresses and hhost names are both valid values.
# The default is 'localhost', which usually means remote machines will not be abble to connect.
# To allow connections from remote users, set this parameter to a non-loopback aaddress.
server.host: "0.0.0.0"

# Enables you to specify a path to mount Kibana at if you are running behind a pproxy. This only affects
# the URLs generated by Kibana, your proxy is expected to remove the basePath vaalue before forwarding requests
# to Kibana. This setting cannot end in a slash.
#server.basePath: ""

# The maximum payload size in bytes for incoming server requests.
#server.maxPayloadBytes: 1048576

# The Kibana server's name.  This is used for display purposes.
#server.name: "your-hostname"
